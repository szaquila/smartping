{
  "$schema": "https://dprint.dev/schemas/v0.json", // dprint 配置文件的 JSON Schema 验证文件路径
  "indentWidth": 2, // 缩进宽度，设置为 2 个空格 (可选值: 数字, 默认: 2)
  "lineWidth": 200, // 行宽限制，超过此宽度的代码会自动换行 (可选值: 数字, 默认: 120)
  "useTabs": true, // 使用制表符（Tab）而不是空格进行缩进 (可选值: true/false, 默认: false)
  "includes": [ // 指定要格式化的文件包含模式
    "**/*.{html,jinja2,ts,tsx,js,jsx,json,md,yml,yaml,sql,toml,py,css,scss,sass,less}" // 匹配所有支持的文件类型
  ],
  "excludes": [ // 指定要排除的文件或目录模式
    "**/node_modules", // 排除 node_modules 目录
    "**/*-lock.json", // 排除所有以 -lock.json 结尾的文件（如 package-lock.json）
    "**/dist", // 排除构建目录
    "**/build", // 排除构建目录
    "**/.git" // 排除 Git 目录
  ],
  "plugins": [ // 配置使用的插件列表
    "../../dprint/dprint_plugin_json.wasm", // JSON 格式化插件
    "../../dprint/dprint_plugin_malva.wasm", // CSS/SCSS/Less 格式化插件（Malva）
    "../../dprint/dprint_plugin_markdown.wasm", // Markdown 格式化插件
    "../../dprint/dprint_plugin_markup.wasm", // HTML/XML/标记语言格式化插件
    "../../dprint/dprint_plugin_ruff.wasm", // Python 格式化插件（基于 Ruff）
    "../../dprint/dprint_plugin_sql.wasm", // SQL 格式化插件
    "../../dprint/dprint_plugin_toml.wasm", // TOML 格式化插件
    "../../dprint/dprint_plugin_typescript.wasm", // TypeScript/JavaScript 格式化插件
    "../../dprint/dprint_plugin_yaml.wasm" // YAML 格式化插件
  ],
  "json": { // JSON 格式化配置
    "associations": [ // JSON 文件关联模式
      "**/*.json" // 匹配所有 JSON 文件
    ],
    "commentLine.forceSpaceAfterSlashes": true, // 强制在注释斜杠后添加空格 (可选值: true/false, 默认: true)
    "indentWidth": 2, // 缩进宽度 (可选值: 数字, 默认: 2)
    "lineWidth": 200, // 行宽限制 (可选值: 数字, 默认: 120)
    "newLineKind": "crlf", // 换行符类型 (可选值: "lf"/"crlf"/"cr", 默认: "lf")
    "preferSingleLine": true, // 优先单行 (可选值: true/false, 默认: false) // 是否优先使用单行格式 (可选值: true/false, 默认: false)
    "trailingCommas": "never", // 尾随逗号策略 (可选值: "never"/"always"/"multiLine", 默认: "never")
    "useTabs": false, // 是否使用制表符 (可选值: true/false, 默认: false)
  },
